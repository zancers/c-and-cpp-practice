#include <iostream>

using namespace std;

int main(){
        long c1,c2;
        cin>>c1>>c2;
        long t=(c2-c1)/100;
        long tag=(c2-c1)%100;
        if(tag>=50)
                t=t+1;
        long h=t/3600;
        long min=t%3600/60;
        long s=t%3600%60;
        //cout<<h<<":"<<min<<":"<<s;
        printf("%02d:%02d:%02d",h,min,s);
        return 0;
}
